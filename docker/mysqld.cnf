[mysqld]
sql_mode = ""

key_buffer_size		= 16M
max_allowed_packet	= 16M
thread_stack		= 192K
thread_cache_size       = 8

innodb_flush_log_at_trx_commit = 2
innodb_flush_method = O_DIRECT
transaction-isolation = READ-COMMITTED

query_cache_limit	= 1M
query_cache_size        = 16M

expire_logs_days	= 10
max_binlog_size   = 100M